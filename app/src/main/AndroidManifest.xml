<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kirandeep.subtractiontutor">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Database.HomeActivity" android:hardwareAccelerated ="false">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".PracticeHome" />
        <activity android:name=".Database.SignUPActivity" />
        <activity android:name=".Audio.TextToSpeak" />
        <activity android:name=".TutorialsHome" />
        <activity android:name=".Counting"></activity>
        <activity android:name=".OptionsScreen"/>
        <activity android:name=".ResultShow" android:theme="@android:style/Theme.Dialog"/>
    </application>

</manifest>
